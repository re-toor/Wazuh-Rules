<decoder name="bvbgate">
  <program_name>BVBGate</program_name>
</decoder>

<decoder name="bvbgate">
  <prematch>^bvbgate</prematch>
</decoder>

<decoder name="bvbgate">
  <parent>bvbgate</parent>
  <regex>[(\.*)\|(\.*)] (\.*) \w+=</regex>
  <order>event.description,severity,event.name</order>
</decoder>
        
