<decoder name="trend_micro_IPS">
    <prematch>BVB-TRENDMICRO-SMS</prematch>
</decoder>

<decoder name="trend_micro_IPS">
    <parent>trend_micro_IPS</parent>
    <regex>BVB-TRENDMICRO-SMS (\.+)\|(\.+)\|(\d+)\|(\.+)\|(\.+)\|(\.+)\|</regex>
    <order>suser,srcip,event.id,type,event.status,description</order>
</decoder>

